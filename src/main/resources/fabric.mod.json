{
  "schemaVersion": 1,
  "id": "deathcounter",
  "version": "${version}",
  "name": "Death Counter",
  "description": "Death Counter with OpenAPI and client Web UI",
  "authors": [
    "Syorito Hatsuki",
    "Nikson"
  ],
  "contact": {
    "homepage": "https://syoritohatsuki.dev/"
  },
  "license": "MIT Licence",
  "icon": "assets/deathcounter/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "dev.syoritohatsuki.deathcounter.DeathCounter"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "dev.syoritohatsuki.deathcounter.client.DeathCounterClient"
      }
    ]
  },
  "custom": {
    "duckyupdater": {
      "modrinthId": "7x0zk3YH"
    }
  },
  "mixins": [
    "deathcounter.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric": "*",
    "fabric-language-kotlin": "*",
    "minecraft": ">=1.20",
    "java": ">=17"
  }
}
