{
  "schemaVersion": 1,
  "id": "deathcounter",
  "version": "${version}",
  "name": "Death Counter",
  "description": "Death Counter with OpenAPI and client Web UI",
  "authors": [
    "Syorito Hatsuki",
    "Nikson"
  ],
  "contact": {
    "homepage": "https://syorito-hatsuki.dev/",
    "discord": "https://discord.gg/pbwnMwnUD6"
  },
  "license": "MIT Licence",
  "icon": "assets/deathcounter/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "dev.syoritohatsuki.deathcounter.DeathCounter"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "dev.syoritohatsuki.deathcounter.client.DeathCounterClient"
      }
    ]
  },
  "custom": {
    "duckyupdater": {
      "modrinthId": "7x0zk3YH"
    },
    "fstats": 34,
    "mcb": [
      {
        "name": "Ducky House",
        "fillColor": -13730510,
        "outlineColor": -11751600
      }
    ]
  },
  "mixins": [
    "deathcounter.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.20",
    "fabric": "*",
    "fabric-language-kotlin": "*",
    "minecraft": ">=1.21",
    "java": ">=21"
  }
}
